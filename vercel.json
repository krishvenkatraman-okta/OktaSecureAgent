{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "functions": {
    "api/simple.ts": {
      "runtime": "@vercel/node@18.x"
    }
  },
  "routes": [
    {
      "src": "/api/simple",
      "dest": "/api/simple"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}